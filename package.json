    "overrides": {
      "@testing-library/react>@types/react": "$@types/react",
      "@testing-library/react>@types/react-dom": "$@types/react-dom",
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
