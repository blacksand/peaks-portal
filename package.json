{
  "name": "peaks",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nx dev",
    "build": "nx build",
    "test": "nx test",
    "check-types": "nx check-types",
    "prepare": "husky",
    "release": "commit-and-tag-version",
    "format": "prettier --write \"**/*.{css,scss,json,md}\""
  },
  "dependencies": {
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-nx-scopes": "^19.5.0",
    "@eslint-react/eslint-plugin": "^1.14.2",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-react-query": "^6.1.0",
    "@nx/devkit": "19.8.0",
    "@nx/eslint": "19.8.0",
    "@nx/eslint-plugin": "19.8.0",
    "@nx/js": "19.8.0",
    "@nx/next": "19.8.0",
    "@nx/playwright": "19.8.0",
    "@nx/plugin": "19.8.0",
    "@nx/react": "19.8.0",
    "@nx/storybook": "19.8.0",
    "@nx/vite": "19.8.0",
    "@nx/web": "19.8.0",
    "@parcel/watcher": "^2.4.1",
    "@peaks/config-eslint": "workspace:*",
    "@peaks/config-tailwind": "workspace:*",
    "@peaks/config-testing": "workspace:*",
    "@playwright/test": "^1.47.2",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@tanstack/eslint-plugin-query": "^5.57.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20.16.5",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@typescript-eslint/utils": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "eslint": "~9.11.0",
    "eslint-config-next": "catalog:next",
    "eslint-config-prettier": "^9.1.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-formatter-summary": "^1.1.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "7.36.1",
    "eslint-plugin-react-hooks": "5.1.0-beta-26f2496093-20240514",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-readable-tailwind": "^1.8.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "nx": "19.8.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-brace-style": "^0.7.0",
    "prettier-plugin-css-order": "^2.1.2",
    "shadcn": "^2.0.7",
    "storybook": "^8.3.2",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "ts-essentials": "^10.0.2",
    "typescript": "~5.5.4",
    "vite": "^5.4.7",
    "vitest": "^2.1.1"
  },
  "pnpm": {
    "supportedArchitectures": {
      "libc": [
        "glibc",
        "musl"
      ]
    },
    "overrides": {
      "@nx/vite>vitest": "$vitest",
      "@testing-library/react>@types/react": "npm:types-react@19.0.0-rc.1",
      "@testing-library/react>@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
      "eslint": "$eslint",
      "eslint-plugin-react-hooks": "$eslint-plugin-react-hooks",
      "react": "catalog:react",
      "react-dom": "catalog:react",
      "react-is": "catalog:react"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react",
        "@types/react-dom"
      ]
    }
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs,ts,tsx,md,mdx,json}": [
      "nx affected:lint --fix --files"
    ],
    "**/*.{md,mdx}": [
      "nx format:write --files",
      "nx affected:lint --fix --files"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix",
      "nx format:write --files"
    ]
  }
}
