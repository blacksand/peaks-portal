      "eslint": "$eslint",
      "eslint-plugin-react-hooks": "$eslint-plugin-react-hooks",
      "react": "$react",
      "react-dom": "$react-dom",
      "scheduler": "0.25.0-rc-5dcb0097-20240918"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react",
        "@types/react-dom"
      ]
    }
  },
  "lint-staged": {
    "**/*.{js,cjs,mjs,ts,tsx,md,mdx,json}": [
      "nx affected:lint --fix --files"
    ],
    "**/*.{md,mdx}": [
      "nx format:write --files",
      "nx affected:lint --fix --files"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix",
      "nx format:write --files"
    ]
